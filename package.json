{
  "name": "template-intelligence-engine",
  "version": "1.0.0",
  "description": "AI-powered template processor for financial advisory documents",
  "scripts": {
    "setup": "npm install && cd client && npm install && cd ..",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "pizzip": "^3.1.6",
    "docxtemplater": "^3.42.3",
    "jszip": "^3.10.1",
    "xml2js": "^0.6.2",
    "uuid": "^9.0.1",
    "@anthropic-ai/sdk": "^0.20.0",
    "mammoth": "^1.6.0",
    "pdf-parse": "^1.1.1",
    "docx": "^8.5.0"
  
},
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
